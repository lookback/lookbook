{
  "name": "lookbook",
  "version": "2.0.0-beta.6",
  "description": "The design system of Lookback",
  "main": "index.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "lint": "stylelint src/**/*.css",
    "pretest": "npm run lint",
    "test": "[[ -z \"$CI\" ]] && pa11y ./test/test.html || mkdir -p test-output/junit && pa11y -S ./test-output/screenshot.png --config test/pa11y.config.json -r junit ./test/test.html > test-output/junit/results.xml",
    "build": "scripts/build && npm run generate-types && scripts/make-compat",
    "watch": "scripts/build --watch --verbose",
    "update-colors": "scripts/fetch-figma-colors && scripts/write-test-file && npm run generate-types",
    "generate-types": "scripts/generate-types",
    "release": "npm install && npm run build && npm test && git add package.json package-lock.json dist && git commit -m $npm_package_version && git tag $npm_package_version && git push && git push --tags && scripts/distribute \"dist/*.css\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lookback/lookbook.git"
  },
  "keywords": [
    "css",
    "design",
    "system",
    "components"
  ],
  "author": "Johan Brook <johan@lookback.io>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/lookback/lookbook/issues"
  },
  "homepage": "https://github.com/lookback/lookbook#readme",
  "devDependencies": {
    "aws-sdk": "^2.408.0",
    "css-declaration-sorter": "^4.0.1",
    "cssnano-util-raw-cache": "^4.0.1",
    "dotenv": "^6.2.0",
    "eslint": "^5.8.0",
    "fast-glob": "^2.2.7",
    "node-fetch": "^2.3.0",
    "pa11y": "^5.2.0",
    "pa11y-reporter-junit": "^1.1.0",
    "postcss-calc": "^7.0.1",
    "postcss-cli": "^6.1.3",
    "postcss-colormin": "^4.0.3",
    "postcss-convert-values": "^4.0.1",
    "postcss-discard-comments": "^4.0.2",
    "postcss-discard-duplicates": "^4.0.2",
    "postcss-discard-empty": "^4.0.1",
    "postcss-discard-overridden": "^4.0.1",
    "postcss-merge-longhand": "^4.0.11",
    "postcss-merge-rules": "^4.0.3",
    "postcss-minify-font-values": "^4.0.2",
    "postcss-minify-gradients": "^4.0.2",
    "postcss-minify-params": "^4.0.2",
    "postcss-minify-selectors": "^4.0.2",
    "postcss-normalize-charset": "^4.0.1",
    "postcss-normalize-display-values": "^4.0.2",
    "postcss-normalize-positions": "^4.0.2",
    "postcss-normalize-repeat-style": "^4.0.2",
    "postcss-normalize-string": "^4.0.2",
    "postcss-normalize-timing-functions": "^4.0.2",
    "postcss-normalize-unicode": "^4.0.1",
    "postcss-normalize-url": "^4.0.1",
    "postcss-normalize-whitespace": "^4.0.2",
    "postcss-ordered-values": "^4.1.2",
    "postcss-reduce-initial": "^4.0.3",
    "postcss-reduce-transforms": "^4.0.2",
    "postcss-unique-selectors": "^4.0.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1",
    "stylelint-config-recommended": "^2.2"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "postcss": "^7.0.14",
    "postcss-color-function": "^4.0.1",
    "postcss-color-hex-alpha": "^5.0.2",
    "postcss-hexrgba": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.1",
    "tailwindcss": "^1.0.3"
  }
}
